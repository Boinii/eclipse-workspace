package Com.decisionMaking;

public class IfClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//if class
		int var1=34;
		System.out.println("The value of the var1 is:- "+var1);
		int var2=23;
		System.out.println("The value of the var2 is :- "+var2);
		if(var1>var2) {
			System.out.println("The var1 is grater then the var2   "+var1);
			System.out.println("The var2 is lesser then the var1  "+var1);
		}
		
	
	}

}
