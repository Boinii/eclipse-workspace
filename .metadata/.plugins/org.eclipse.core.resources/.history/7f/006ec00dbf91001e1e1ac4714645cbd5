package Com.Loops;

public class ForLoopExamples {

	public static void main(String[] args) {
		
		for(int i=0;i<3;i++) {
			System.out.println(i+"Selenium");
		}
		System.out.println("===========================");
		for(int i=1;i<=3;i++) {
			System.out.println(i+"Manual tsting ");
		}
		System.out.println("==========================");
		for(int i=3;i>1;i++) {
			System.out.println(i+"Automation Testing ");
		}
		
	}

}
